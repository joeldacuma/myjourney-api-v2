{
  "kind": "collectionType",
  "collectionName": "yrock_members",
  "info": {
    "singularName": "yrock-member",
    "pluralName": "yrock-members",
    "displayName": "Yrock Members"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "middleName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "password",
      "required": true,
      "private": true
    },
    "birthDate": {
      "type": "date"
    },
    "profileImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "organization": {
      "type": "string",
      "default": "yrock",
      "required": true
    },
    "role": {
      "type": "enumeration",
      "enum": [
        "Yrock Mentor",
        "Yrock Lifegroup Leader",
        "Yrock Mentee"
      ],
      "default": "Yrock Mentee",
      "required": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false
    },
    "blocked": {
      "type": "boolean",
      "default": false
    }
  }
}
