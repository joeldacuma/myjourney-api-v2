{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Events"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "date": {
      "type": "date"
    },
    "startTime": {
      "type": "time"
    },
    "endTime": {
      "type": "time"
    },
    "description": {
      "type": "text"
    },
    "zone": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::location.location"
    },
    "address": {
      "type": "string"
    },
    "contact": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "pastoralCare": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::designation.designation"
    }
  }
}
