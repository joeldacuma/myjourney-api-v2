{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "organisation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organisation.organisation"
    },
    "position": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::position.position"
    },
    "designation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::designation.designation"
    },
    "assembly": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::assembly.assembly"
    },
    "firstName": {
      "type": "string"
    },
    "middleName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "birthdate": {
      "type": "date"
    },
    "userId": {
      "type": "string",
      "required": true
    },
    "profileImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "certification": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
